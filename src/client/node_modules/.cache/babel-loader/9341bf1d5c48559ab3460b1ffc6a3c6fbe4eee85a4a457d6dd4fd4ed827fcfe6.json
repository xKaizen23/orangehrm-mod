{"ast":null,"code":"import { APIService } from '@ohrm/core/util/services/api.service';\nexport default {\n  name: 'CustomerAutocomplete',\n  setup() {\n    const http = new APIService(window.appGlobal.baseUrl, '/api/v2/time/customers');\n    return {\n      http\n    };\n  },\n  methods: {\n    async loadCustomers(serachParam) {\n      return new Promise(resolve => {\n        if (serachParam.trim()) {\n          this.http.getAll({\n            name: serachParam.trim()\n          }).then(({\n            data\n          }) => {\n            resolve(data.data.map(customer => {\n              return {\n                id: customer.id,\n                label: customer.name\n              };\n            }));\n          });\n        } else {\n          resolve([]);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAiCA,SAAQA,UAAU,QAAO,sCAAsC;AAC/D,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,KAAK,GAAG;IACN,MAAMC,IAAG,GAAI,IAAIH,UAAU,CACzBI,MAAM,CAACC,SAAS,CAACC,OAAO,EACxB,wBAAwB,CACzB;IACD,OAAO;MACLH;IACF,CAAC;EACH,CAAC;EACDI,OAAO,EAAE;IACP,MAAMC,aAAa,CAACC,WAAW,EAAE;MAC/B,OAAO,IAAIC,OAAO,CAAEC,OAAO,IAAK;QAC9B,IAAIF,WAAW,CAACG,IAAI,EAAE,EAAE;UACtB,IAAI,CAACT,IAAG,CACLU,MAAM,CAAC;YACNZ,IAAI,EAAEQ,WAAW,CAACG,IAAI;UACxB,CAAC,EACAE,IAAI,CAAC,CAAC;YAACC;UAAI,CAAC,KAAK;YAChBJ,OAAO,CACLI,IAAI,CAACA,IAAI,CAACC,GAAG,CAAEC,QAAQ,IAAK;cAC1B,OAAO;gBACLC,EAAE,EAAED,QAAQ,CAACC,EAAE;gBACfC,KAAK,EAAEF,QAAQ,CAAChB;cAClB,CAAC;YACH,CAAC,CAAC,CACH;UACH,CAAC,CAAC;QACN,OAAO;UACLU,OAAO,CAAC,EAAE,CAAC;QACb;MACF,CAAC,CAAC;IACJ;EACF;AACF,CAAC","names":["APIService","name","setup","http","window","appGlobal","baseUrl","methods","loadCustomers","serachParam","Promise","resolve","trim","getAll","then","data","map","customer","id","label"],"sourceRoot":"","sources":["C:\\xampp\\htdocs\\orangehrm-5.7\\src\\client\\src\\orangehrmTimePlugin\\components\\CustomerAutocomplete.vue"],"sourcesContent":["<!--\n/**\n * OrangeHRM is a comprehensive Human Resource Management (HRM) System that captures\n
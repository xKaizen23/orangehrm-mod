umber,\n      required: true,\n    },\n  },\n  setup(props) {\n    const http = new APIService(\n      window.appGlobal.baseUrl,\n      '/api/v2/admin/licenses',\n    );\n    const {createUniqueValidator} = useServerValidation(http);\n    const licenseUniqueValidation = createUniqueValidator('License', 'name', {\n      entityId: props.licenseId,\n    });\n\n    return {\n      http,\n      licenseUniqueValidation,\n    };\n  },\n\n  data() {\n    return {\n      isLoading: false,\n      license: {\n        id: '',\n        name: '',\n      },\n      rules: {\n        name: [\n          required,\n          shouldNotExceedCharLength(100),\n          this.licenseUniqueValidation,\n        ],\n      },\n    };\n  },\n\n  created() {\n    this.isLoading = true;\n    this.http\n      .get(this.licenseId)\n      .then((response) => {\n        const {data} = response.data;\n        this.license.id = data.id;\n        this.license.name = data.name;\n      })\n      .finally(() => {\n        this.isLoading = false;\n      });\n  },\n\n  methods: {\n    onSave() {\n      this.isLoading = true;\n      this.http\n        .update(this.licenseId, {\n          name: this.license.name,\n        })\n        .then(() => {\n          return this.$toast.updateSuccess();\n        })\n        .then(() => {\n          this.onCancel();\n        });\n    },\n    onCancel() {\n      navigate('/admin/viewLicenses');\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"import { APIService } from '@/core/util/services/api.service';\nimport { OxdDialog, OxdSpinner } from '@ohrm/oxd';\nexport default {\n  components: {\n    'oxd-loading-spinner': OxdSpinner,\n    'oxd-dialog': OxdDialog\n  },\n  emits: ['close'],\n  setup() {\n    const http = new APIService(window.appGlobal.baseUrl, '/api/v2/core/about');\n    return {\n      http\n    };\n  },\n  data() {\n    return {\n      isLoading: false,\n      data: null\n    };\n  },\n  beforeMount() {\n    this.isLoading = true;\n    this.http.getAll().then(response => {\n      const {\n        data\n      } = response.data;\n      this.data = {\n        ...data\n      };\n    }).finally(() => {\n      this.isLoading = false;\n    });\n  },\n  methods: {\n    onCancel() {\n      this.$emit('close', true);\n    }\n  }\n};","map":{"version":3,"mappings":"AAmFA,SAAQA,UAAU,QAAO,kCAAkC;AAC3D,SAAQC,SAAS,EAAEC,UAAU,QAAO,WAAW;AAE/C,eAAe;EACbC,UAAU,EAAE;IACV,qBAAqB,EAAED,UAAU;IACjC,YAAY,EAAED;EAChB,CAAC;EACDG,KAAK,EAAE,CAAC,OAAO,CAAC;EAChBC,KAAK,GAAG;IACN,MAAMC,IAAG,GAAI,IAAIN,UAAU,CAACO,MAAM,CAACC,SAAS,CAACC,OAAO,EAAE,oBAAoB,CAAC;IAC3E,OAAO;MACLH;IACF,CAAC;EACH,CAAC;EACDI,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBD,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDE,WAAW,GAAG;IACZ,IAAI,CAACD,SAAQ,GAAI,IAAI;IACrB,IAAI,CAACL,IAAG,CACLO,MAAM,EAAC,CACPC,IAAI,CAAEC,QAAQ,IAAK;MAClB,MAAM;QAACL;MAAI,IAAIK,QAAQ,CAACL,IAAI;MAC5B,IAAI,CAACA,IAAG,GAAI;QAAC,GAAGA;MAAI,CAAC;IACvB,CAAC,EACAM,OAAO,CAAC,MAAM;MACb,IAAI,CAACL,SAAQ,GAAI,KAAK;IACxB,CAAC,CAAC;EACN,CAAC;EACDM,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,IAAI,CAACC,KAAK,CAAC,OAAO,EAAE,IAAI,CAAC;IAC3B;EACF;AACF,CAAC","names":["APIService","OxdDialog","OxdSpinner","components","emits","setup","http","window","appGlobal","baseUrl","data","isLoading","beforeMount","getAll","then","response","finally","methods","onCancel","$emit"],"sourceRoot":"","sources":["C:\\xampp\\htdocs\\orangehrm-5.7\\src\\client\\src\\core\\pages\\About.vue"],"sourcesContent":["<!--\n/**\n 
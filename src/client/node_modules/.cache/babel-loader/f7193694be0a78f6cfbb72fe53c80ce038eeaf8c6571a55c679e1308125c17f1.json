n        data: payload\n      }).then(response => {\n        const {\n          data\n        } = response.data;\n        this.modules = data;\n        return this.$toast.saveSuccess();\n      }).finally(() => {\n        this.isLoading = false;\n        reloadPage();\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA+GA,SAAQA,UAAU,QAAO,kCAAkC;AAC3D,SAAQC,UAAU,QAAO,+BAA+B;AACxD,SAAQC,cAAc,QAAO,WAAW;AAExC,MAAMC,YAAW,GAAI;EACnBC,KAAK,EAAE,KAAK;EACZC,GAAG,EAAE,KAAK;EACVC,KAAK,EAAE,KAAK;EACZC,IAAI,EAAE,KAAK;EACXC,WAAW,EAAE,KAAK;EAClBC,WAAW,EAAE,KAAK;EAClBC,WAAW,EAAE,KAAK;EAClBC,MAAM,EAAE,KAAK;EACbC,SAAS,EAAE,KAAK;EAChBC,KAAK,EAAE,KAAK;EACZC,IAAI,EAAE;AACR,CAAC;AAED,eAAe;EACbC,UAAU,EAAE;IACV,kBAAkB,EAAEb;EACtB,CAAC;EACDc,KAAK,GAAG;IACN,MAAMC,IAAG,GAAI,IAAIjB,UAAU,CACzBkB,MAAM,CAACC,SAAS,CAACC,OAAO,EACxB,uBAAuB,CACxB;IACD,OAAO;MACLH;IACF,CAAC;EACH,CAAC;EACDI,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QAAC,GAAGnB;MAAY,CAAC;MAC1BoB,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACD,SAAQ,GAAI,IAAI;IACrB,IAAI,CAACN,IAAG,CACLQ,MAAM,EAAC,CACPC,IAAI,CAAEC,QAAQ,IAAK;MAClB,MAAM;QAACN;MAAI,IAAIM,QAAQ,CAACN,IAAI;MAC5B,IAAI,CAACC,OAAO,CAAClB,KAAI,GAAIiB,IAAI,CAACjB,KAAK;MAC/B,IAAI,CAACkB,OAAO,CAACjB,GAAE,GAAIgB,IAAI,CAAChB,GAAG;MAC3B,IAAI,CAACiB,OAAO,CAAChB,KAAI,GAAIe,IAAI,CAACf,KAAK;MAC/B,IAAI,CAACgB,OAAO,CAACf,IAAG,GAAIc,IAAI,CAACd,IAAI;MAC7B,IAAI,CAACe,OAAO,CAACd,WAAU,GAAIa,IAAI,CAACb,WAAW;MAC3C,IAAI,CAACc,OAAO,CAACb,WAAU,GAAIY,IAAI,CAACZ,WAAW;MAC3C,IAAI,CAACa,OAAO,CAACZ,WAAU,GAAIW,IAAI,CAACX,WAAW;MAC3C,IAAI,CAACY,OAAO,CAACX,MAAK,GAAIU,IAAI,CAACV,MAAM;MACjC,IAAI,CAACW,OAAO,CAACV,SAAQ,GAAIS,IAAI,CAACT,SAAS;MACvC,IAAI,CAACU,OAAO,CAACT,KAAI,GAAIQ,IAAI,CAACR,KAAK;MAC/B,IAAI,CAACS,OAAO,CAACR,IAAG,GAAIO,IAAI,CAACP,IAAI;IAC/B,CAAC,EACAc,OAAO,CAAC,MAAM;MACb,IAAI,CAACL,SAAQ,GAAI,KAAK;IACxB,CAAC,CAAC;EACN,CAAC;EACDM,OAAO,EAAE;IACPC,MAAM,GAAG;MACP,IAAI,CAACP,SAAQ,GAAI,IAAI;MACrB,MAAMQ,OAAM,GAAI;QACd3B,KAAK,EAAE,IAAI;QACXC,GAAG,EAAE,IAAI;QACTC,KAAK,EAAE,IAAI,CAACgB,OAAO,CAAChB,K
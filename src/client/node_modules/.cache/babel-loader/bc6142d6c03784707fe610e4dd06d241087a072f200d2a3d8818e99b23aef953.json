{"ast":null,"code":"import { urlFor } from '@ohrm/core/util/helper/url';\nimport { navigate } from '@/core/util/helper/navigation';\nimport { required } from '@/core/util/validation/rules';\nimport { OxdAlert } from '@ohrm/oxd';\nexport default {\n  name: 'AdministratorAccess',\n  components: {\n    'oxd-alert': OxdAlert\n  },\n  props: {\n    username: {\n      type: String,\n      required: true\n    },\n    error: {\n      type: Object,\n      default: () => null\n    },\n    token: {\n      type: String,\n      required: true\n    },\n    backUrl: {\n      type: String,\n      default: null\n    }\n  },\n  data() {\n    return {\n      password: '',\n      rules: {\n        password: [required]\n      },\n      noteClasses: {\n        'orangehrm-admin-access-note': true,\n        '--padding': this.error === null\n      },\n      noteContainerClass: {\n        'orangehrm-admin-access-note-container': this.error === null\n      }\n    };\n  },\n  computed: {\n    submitUrl() {\n      return urlFor('/auth/adminVerify');\n    }\n  },\n  methods: {\n    onSubmit() {\n      this.$refs.verifyForm.$el.submit();\n    },\n    onCancel() {\n      navigate(this.backUrl);\n    }\n  }\n};","map":{"version":3,"mappings":"AA4FA,SAAQA,MAAM,QAAO,4BAA4B;AACjD,SAAQC,QAAQ,QAAO,+BAA+B;AACtD,SAAQC,QAAQ,QAAO,8BAA8B;AACrD,SAAQC,QAAQ,QAAO,WAAW;AAElC,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAE3BC,UAAU,EAAE;IACV,WAAW,EAAEF;EACf,CAAC;EAEDG,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,MAAM;MACZP,QAAQ,EAAE;IACZ,CAAC;IACDQ,KAAK,EAAE;MACLF,IAAI,EAAEG,MAAM;MACZC,OAAO,EAAE,MAAM;IACjB,CAAC;IACDC,KAAK,EAAE;MACLL,IAAI,EAAEC,MAAM;MACZP,QAAQ,EAAE;IACZ,CAAC;IACDY,OAAO,EAAE;MACPN,IAAI,EAAEC,MAAM;MACZG,OAAO,EAAE;IACX;EACF,CAAC;EAEDG,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;QACLD,QAAQ,EAAE,CAACd,QAAQ;MACrB,CAAC;MACDgB,WAAW,EAAE;QACX,6BAA6B,EAAE,IAAI;QACnC,WAAW,EAAE,IAAI,CAACR,KAAI,KAAM;MAC9B,CAAC;MACDS,kBAAkB,EAAE;QAClB,uCAAuC,EAAE,IAAI,CAACT,KAAI,KAAM;MAC1D;IACF,CAAC;EACH,CAAC;EAEDU,QAAQ,EAAE;IACRC,SAAS,GAAG;MACV,OAAOrB,MAAM,CAAC,mBAAmB,CAAC;IACpC;EACF,CAAC;EAEDsB,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,IAAI,CAACC,KAAK,CAACC,UAAU,CAACC,GAAG,CAACC,MAAM,EAAE;IACpC,CAAC;IACDC,QAAQ,GAAG;MACT3B,QAAQ,CAAC,IAAI,CAACa,OAAO,CAAC;IACxB;EACF;AACF,CAAC","names":["urlFor","navigate","required","OxdAlert","name","components","props","username","type","String","error","Object","default","token","backUrl","data","password","rules","noteClasses","noteContainerClass","computed","submitUrl","methods","onSubmit","$refs","verifyForm","$el","submit","onCancel"],"sourceRoot":"","sources":["C:\\xampp\\htdocs\\orangehrm-5.7\\src\\client\\src\\orangehrmAuthenticationPlugin\\pages\\AdministratorAccess.vue"],"sourcesContent":["<!--\n/**\n * OrangeHRM is a comprehensive Human Resource Management (HRM) System that captures\n * all the essential functionalities required for any enterprise.\n * Copyright (C) 2006 OrangeHRM Inc., http://www.orangehrm.com\n *\n * OrangeHRM is free software: you can redistribute it and/or modify it under the terms of\n * the GNU General Public License as published by the Free Software Foundation, either\n * version 3 of the License, or (at your option) any later version.\n *\n * OrangeHRM is distributed in the hope that it will be useful, but WITHOUT ANY WARRANTY;\n * without even the implied warranty of MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.\n * See the GNU General Public License for more details.\n *\n * You should have received a copy of the GNU General Public License along with OrangeHRM.\n * If not, see <https://www.gnu.org/licenses/>.\n */\n -->\n\n<template>\n  <div class=\"orangehrm-admin-access-container\">\n    <div class=\"orangehrm-card-container\">\n      <oxd-form\n        ref=\"verifyForm\"\n        method=\"post\"\n        :action=\"submitUrl\"\n        @submit-valid=\"onSubmit\"\n      >\n        <oxd-text tag=\"h6\" class=\"orangehrm-admin-access-title\">\n          {{ $t('auth.admin_access') }}\n        </oxd-text>\n\n        <br />\n\n        <div :class=\"noteContainerClass\">\n          <oxd-text tag=\"toast-message\">\n            {{ $t('auth.admin_access_note') }}\n          </oxd-text>\n        </div>\n\n        <oxd-alert\n          :show=\"error !== null\"\n          type=\"error\"\n          :message=\"error?.message\"\n        />\n\n        <input name=\"_token\" :value=\"token\" type=\"hidden\" />\n\n        <oxd-form-row>\n          <oxd-input-field\n            :model-value=\"username\"\n            name=\"username\"\n            :label=\"$t('general.username')\"\n            label-icon=\"person\"\n            disabled\n          />\n        </oxd-form-row>\n        <oxd-form-row>\n          <oxd-input-field\n            v-model=\"password\"\n            name=\"password\"\n            :label=\"$t('general.password')\"\n            label-icon=\"key\"\n            type=\"password\"\n            :rules=\"rules.password\"\n            autofocus\n          />\n        </oxd-form-row>\n        <div class=\"orangehrm-admin-access-button-container\">\n          <oxd-button\n            class=\"orangehrm-admin-access-button\"\n            display-type=\"ghost\"\n            size=\"large\"\n            :label=\"$t('general.cancel')\"\n            @click=\"onCancel\"\n          />\n          <oxd-button\n            class=\"orangehrm-admin-access-button\"\n            display-type=\"secondary\"\n            size=\"large\"\n            :label=\"$t('general.confirm')\"\n            type=\"submit\"\n          />\n        </div>\n      </oxd-form>\n    </div>\n    <br />\n    <slot name=\"footer\"></slot>\n  </div>\n</template>\n\n<script>\nimport {urlFor} from '@ohrm/core/util/helper/url';\nimport {navigate} from '@/core/util/helper/navigation';\nimport {required} from '@/core/util/validation/rules';\nimport {OxdAlert} from '@ohrm/oxd';\n\nexport default {\n  name: 'AdministratorAccess',\n\n  components: {\n    'oxd-alert': OxdAlert,\n  },\n\n  props: {\n    username: {\n      type: String,\n      required: true,\n    },\n    error: {\n      type: Object,\n      default: () => null,\n    },\n    token: {\n      type: String,\n      required: true,\n    },\n    backUrl: {\n      type: String,\n      default: null,\n    },\n  },\n\n  data() {\n    return {\n      password: '',\n      rules: {\n        password: [required],\n      },\n      noteClasses: {\n        'orangehrm-admin-access-note': true,\n        '--padding': this.error === null,\n      },\n      noteContainerClass: {\n        'orangehrm-admin-access-note-container': this.error === null,\n      },\n    };\n  },\n\n  computed: {\n    submitUrl() {\n      return urlFor('/auth/adminVerify');\n    },\n  },\n\n  methods: {\n    onSubmit() {\n      this.$refs.verifyForm.$el.submit();\n    },\n    onCancel() {\n      navigate(this.backUrl);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.orangehrm-card-container {\n  box-shadow: 3px 3px 10px $oxd-interface-gray-color;\n}\n\n.orangehrm-admin-access {\n  &-container {\n    display: flex;\n    width: inherit;\n    height: inherit;\n    padding: 1rem 2rem;\n    align-items: center;\n    flex-direction: column;\n    justify-content: center;\n    @include oxd-respond-to('md') {\n      margin: 0 auto;\n      max-width: 450px;\n    }\n  }\n  &-title {\n    font-weight: 700;\n  }\n  &-note-container {\n    padding-bottom: 1.2rem;\n  }\n  &-button {\n    flex: 1;\n    &:nth-child(2) {\n      margin-top: 0.5rem;\n      @include oxd-respond-to('md') {\n        margin: 0;\n        margin-left: 0.5rem;\n      }\n    }\n    &-container {\n      display: flex;\n      flex-direction: column;\n      justify-content: center;\n      @include oxd-respond-to('md') {\n        flex-direction: row;\n      }\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
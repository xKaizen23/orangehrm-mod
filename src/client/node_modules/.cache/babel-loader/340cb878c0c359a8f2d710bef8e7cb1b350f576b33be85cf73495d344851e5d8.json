: {\n    onSave() {\n      this.isLoading = true;\n      this.http.create({\n        name: this.holiday.name,\n        date: this.holiday.date,\n        recurring: this.holiday.recurring,\n        length: this.holiday.length.id\n      }).then(() => {\n        return this.$toast.saveSuccess();\n      }).then(() => {\n        this.holiday = {\n          ...holidayModel\n        };\n        this.onCancel();\n      });\n    },\n    onCancel() {\n      navigate('/leave/viewHolidayList');\n    }\n  }\n};","map":{"version":3,"mappings":";AAqGA,SAAQA,QAAQ,QAAO,mCAAmC;AAC1D,SAAQC,UAAU,QAAO,sCAAsC;AAC/D,SACEC,QAAQ,EACRC,yBAAyB,EACzBC,eAAe,QACV,kCAAkC;AACzC,SAAQC,SAAS,QAAO,mCAAmC;AAC3D,OAAOC,aAAY,MAAO,sCAAsC;AAEhE,MAAMC,YAAW,GAAI;EACnBC,IAAI,EAAE,EAAE;EACRC,IAAI,EAAE,EAAE;EACRC,SAAS,EAAE,KAAK;EAChBC,MAAM,EAAE;IAACC,EAAE,EAAE,CAAC;IAAEC,KAAK,EAAE;EAAU;AACnC,CAAC;AAED,eAAe;EACbC,KAAK,EAAE;IACLC,iBAAiB,EAAE;MACjBC,IAAI,EAAEC,KAAK;MACXf,QAAQ,EAAE;IACZ;EACF,CAAC;EAEDgB,KAAK,GAAG;IACN,MAAMC,IAAG,GAAI,IAAIlB,UAAU,CACzBmB,MAAM,CAACC,SAAS,CAACC,OAAO,EACxB,wBAAwB,CACzB;IACD,MAAM;MAACC;IAAc,IAAIjB,aAAa,EAAE;IAExC,OAAO;MACLa,IAAI;MACJI;IACF,CAAC;EACH,CAAC;EAEDC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,CAAC,GAAGpB,SAAS,CAAC,GAAG,CAAC,CAAC;MAC9BqB,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAE;QAAC,GAAGpB;MAAY,CAAC;MAC1BqB,KAAK,EAAE;QACLpB,IAAI,EAAE,CAACN,QAAQ,EAAEC,yBAAyB,CAAC,GAAG,CAAC,CAAC;QAChDM,IAAI,EAAE,CAACP,QAAQ,EAAEE,eAAe,CAAC,IAAI,CAACmB,cAAc,CAAC,CAAC;QACtDZ,MAAM,EAAE,CAACT,QAAQ;MACnB,CAAC;MACD2B,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACJ,SAAQ,GAAI,IAAI;IACrB;IACA,MAAMK,KAAI,GAAI,IAAIC,IAAI,EAAE;IACxB,MAAMC,SAAQ,GACZF,KAAK,CAACG,WAAW,EAAC,GAClB,GAAE,GACF,GAAE,IACDH,KAAK,CAACI,QAAQ,EAAC,GAAI,CAAC,IACrB,GAAE,GACFJ,KAAK,CAACK,OAAO,EAAE;IACjB,MAAMC,OAAM,GACVN,KAAK,CAACG,WAAW,EAAC,GAClB,GAAE,GACF,GAAE,IACDH,KAAK,CAACI,QAAQ,EAAC,GAAI,CAAC,IACrB,GAAE,GACFJ,KAAK,CAACK,OAAO,EAAE;IACjB,IAAI,CAACjB,IAAG,CACLmB,MAAM,CAAC;MAACC,QAAQ,EAAEN,SAAS;MAAEO,MAAM,EAAEH,OAAO;MAAEI,KAAK,EAAE;IAAC,CAAC,EA
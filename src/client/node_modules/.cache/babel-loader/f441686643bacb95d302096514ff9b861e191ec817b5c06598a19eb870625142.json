{"ast":null,"code":"import { navigate } from '@ohrm/core/util/helper/navigation';\nimport { APIService } from '@ohrm/core/util/services/api.service';\nimport { required, shouldNotExceedCharLength } from '@ohrm/core/util/validation/rules';\nimport useServerValidation from '@/core/util/composable/useServerValidation';\nexport default {\n  props: {\n    qualificationMembershipId: {\n      type: Number,\n      required: true\n    }\n  },\n  setup(props) {\n    const http = new APIService(window.appGlobal.baseUrl, '/api/v2/admin/memberships');\n    const {\n      createUniqueValidator\n    } = useServerValidation(http);\n    const membershipUniqueValidation = createUniqueValidator('Membership', 'name', {\n      entityId: props.qualificationMembershipId\n    });\n    return {\n      http,\n      membershipUniqueValidation\n    };\n  },\n  data() {\n    return {\n      isLoading: false,\n      membership: {\n        id: '',\n        name: ''\n      },\n      rules: {\n        name: [required, shouldNotExceedCharLength(50), this.membershipUniqueValidation]\n      }\n    };\n  },\n  created() {\n    this.isLoading = true;\n    this.http.get(this.qualificationMembershipId).then(response => {\n      const {\n        data\n      } = response.data;\n      this.membership.id = data.id;\n      this.membership.name = data.name;\n    }).finally(() => {\n      this.isLoading = false;\n    });\n  },\n  methods: {\n    onSave() {\n      this.isLoading = true;\n      this.http.update(this.qualificationMembershipId, {\n        name: this.membership.name\n      }).then(() => {\n        return this.$toast.updateSuccess();\n      }).then(() => {\n        this.onCancel();\n      });\n    },\n    onCancel() {\n      navigate('/admin/membership');\n    }\n  }\n};","map":{"version":3,"mappings":"AAwDA,SAAQA,QAAQ,QAAO,mCAAmC;AAC1D,SAAQC,UAAU,QAAO,sCAAsC;AAC/D,SACEC,QAAQ,EACRC,yBAAyB,QACpB,kCAAkC;AACzC,OAAOC,mBAAkB,MAAO,4CAA4C;AAE5E,eAAe;EACbC,KAAK,EAAE;IACLC,yBAAyB,EAAE;MACzBC,IAAI,EAAEC,MAAM;MA
{"ast":null,"code":"import { APIService } from '@ohrm/core/util/services/api.service';\nimport LanguagesDropdown from '@/orangehrmAdminPlugin/components/LanguagesDropdown.vue';\nimport { required } from '@/core/util/validation/rules';\nimport { OxdDialog } from '@ohrm/oxd';\nexport default {\n  name: 'AddLanguageModal',\n  components: {\n    'oxd-dialog': OxdDialog,\n    'languages-dropdown': LanguagesDropdown\n  },\n  emits: ['close'],\n  setup() {\n    const http = new APIService(window.appGlobal.baseUrl, '/api/v2/admin/i18n/languages');\n    return {\n      http\n    };\n  },\n  data() {\n    return {\n      isLoading: false,\n      language: null,\n      rules: {\n        language: [required]\n      }\n    };\n  },\n  methods: {\n    onSave() {\n      this.http.update(this.language.id, null).then(response => {\n        if (response) {\n          return this.$toast.saveSuccess();\n        }\n      }).then(() => {\n        this.isLoading = false;\n        this.$emit('close');\n      });\n    },\n    onCancel() {\n      this.$emit('close');\n    }\n  }\n};","map":{"version":3,"mappings":"AAgEA,SAAQA,UAAU,QAAO,sCAAsC;AAC/D,OAAOC,iBAAgB,MAAO,yDAAyD;AACvF,SAAQC,QAAQ,QAAO,8BAA8B;AACrD,SAAQC,SAAS,QAAO,WAAW;AAEnC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,UAAU,EAAE;IACV,YAAY,EAAEF,SAAS;IACvB,oBAAoB,EAAEF;EACxB,CAAC;EACDK,KAAK,EAAE,CAAC,OAAO,CAAC;EAChBC,KAAK,GAAG;IACN,MAAMC,IAAG,GAAI,IAAIR,UAAU,CACzBS,MAAM,CAACC,SAAS,CAACC,OAAO,EACxB,8BAA8B,CAC/B;IAED,OAAO;MACLH;IACF,CAAC;EACH,CAAC;EACDI,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,QAAQ,EAAE,IAAI;MACdC,KAAK,EAAE;QACLD,QAAQ,EAAE,CAACZ,QAAQ;MACrB;IACF,CAAC;EACH,CAAC;EACDc,OAAO,EAAE;IACPC,MAAM,GAAG;MACP,IAAI,CAACT,IAAG,CACLU,MAAM,CAAC,IAAI,CAACJ,QAAQ,CAACK,EAAE,EAAE,IAAI,EAC7BC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAIA,QAAQ,EAAE;UACZ,OAAO,IAAI,CAACC,MAAM,CAACC,WAAW,EAAE;QAClC;MACF,CAAC,EACAH,IAAI,CAAC,MAAM;QACV,IAAI,CAACP,SAAQ,GAAI,KAAK;QACtB,IAAI,CAACW,KAAK,CAAC,OAAO,CAAC;MACrB,CAAC,CAAC;IACN,CAAC;IACDC,QAAQ,GAAG;MACT,IAAI,CAACD,KAAK,CAA
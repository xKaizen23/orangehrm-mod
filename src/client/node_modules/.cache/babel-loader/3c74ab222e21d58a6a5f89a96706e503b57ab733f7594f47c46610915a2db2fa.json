ls.\n *\n * You should have received a copy of the GNU General Public License along with OrangeHRM.\n * If not, see <https://www.gnu.org/licenses/>.\n */\n -->\n\n<template>\n  <oxd-input-field\n    type=\"autocomplete\"\n    :clear=\"false\"\n    :label=\"$t('claim.reference_id')\"\n    :create-options=\"loadTypes\"\n  >\n    <template #option=\"{data}\">\n      <span> {{ data.label }} </span>\n    </template>\n  </oxd-input-field>\n</template>\n\n<script>\nimport {APIService} from '@ohrm/core/util/services/api.service';\nexport default {\n  name: 'ReferenceIdAutocomplete',\n  props: {\n    isAssigned: {\n      default: false,\n      type: Boolean,\n      required: false,\n    },\n  },\n  setup(props) {\n    const http = new APIService(\n      window.appGlobal.baseUrl,\n      props.isAssigned\n        ? '/api/v2/claim/employees/requests?model=summary'\n        : '/api/v2/claim/requests?model=summary',\n    );\n    return {\n      http,\n    };\n  },\n  methods: {\n    async loadTypes(serachParam) {\n      return new Promise((resolve) => {\n        if (serachParam.trim()) {\n          const params = {\n            referenceId: serachParam.trim(),\n          };\n          this.http.getAll(params).then(({data}) => {\n            resolve(\n              data.data.map((claimRequest) => {\n                return {\n                  id: claimRequest.referenceId,\n                  label: claimRequest.referenceId,\n                };\n              }),\n            );\n          });\n        } else {\n          resolve([]);\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\nspan {\n  word-break: break-word;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
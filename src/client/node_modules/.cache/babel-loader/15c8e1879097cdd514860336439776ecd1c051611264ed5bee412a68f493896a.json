, readonly(props.dateFormat));\n\n    const openAboutModel = () => {\n      showAboutModel.value = true;\n    };\n\n    const closeAboutModel = () => {\n      showAboutModel.value = false;\n    };\n\n    const onClickSupport = () => {\n      if (props.helpUrl) window.open(props.helpUrl, '_blank');\n    };\n\n    return {\n      onClickSupport,\n      showAboutModel,\n      openAboutModel,\n      closeAboutModel,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.orangehrm-upgrade-layout {\n  .oxd-topbar-header-userarea {\n    align-self: center;\n    margin-left: unset;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}